(define (bash:command x) (cadr (system x)))
(define (bash:ls) (bash:command "ls"))
(define (bash:pwd) (bash:command "pwd"))
(define (bash:uname) (bash:command "uname"))
(define (bash:date) (bash:command "date"))

(define (bash:cd . dir) (car (system "cd" dir)))
(define (bash:ps . args) (cadr (system "ps" args)))
(define (bash:unix-timestamp) (string->number (cadr (system "date" ["+%s"]))))
